{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/hooks": "^1.8.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.13",
    "antd-mobile": "^2.3.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "qs": "^6.9.2",
    "rc-form": "^2.4.11",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.0.13",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/qs": "^6.9.1"
  }
}
